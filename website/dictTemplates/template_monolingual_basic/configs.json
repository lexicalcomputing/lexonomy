{    
    "ident": {
        "title": "Template: Monolingual basic",
        "blurb": "This is a template for a basic monolingual dictionary suitable for glossaries, terminology lists and small dictionaries. The entry structure is simple and only includes the headword, part of speech, definition and example.\n\nUse the **Dictionary > Configuration > Structure** menu **to** edit the entry structure and add or remove components.\n\nSome entries have been inserted as examples.",
        "lang": "",
        "direction": "ltr"
    },
    "structure": {
        "root": "entry",
        "mode": "custom",
        "tab": "visual",
        "DMLexSettings": {
            "modules": [],
            "xlingual_langs": [],
            "etymology_langs": [],
            "selectedElements": []
        },
        "hasNewEntryTemplate": true,
        "newEntryTemplate": {
            "defaultElements": {
                "entry.pos": true,
                "entry": true,
                "entry.transcription": true,
                "entry.sense": true,
                "entry.sense.definition": true,
                "entry.sense.example": true
            },
            "defaultValues": {}
        }
    },
    "formatting": {
        "elements": {
            "entry": {
                "textsize": 5,
                "colour": "blue",
                "weight": "bold"
            },
            "entry.pos": {},
            "entry.transcription": {
                "lineBreak": "both",
                "outerPunc": "squareBrackets"
            },
