        "sortDesc": false,
        "numberEntries": "1000",
        "locale": "",
        "headwordAnnotationsType": "advanced",
        "headwordAnnotationsAdvanced": "%(entry) <small style=\"color:darkgrey\">%(entry.pos) </small>"
    }
